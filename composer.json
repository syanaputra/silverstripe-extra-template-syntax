{
  "name": "syanaputra/silverstripe-extra-template-syntax",
  "description": "Extra syntax for SilverStripe templating system.",
  "homepage": "http://github.com/my-vendor/my-module",
  "keywords": [
    "silverstripe",
    "template",
    "ss5"
  ],
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Stephanus Yanaputra",
      "email": "stephanus.yanaputra@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/syanaputra/silverstripe-extra-template-syntax/issues"
  },
  "require": {
    "php": "^8.1",
    "silverstripe/cms": "^5.0",
    "silverstripe/framework": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
    "silverstripe/upgrader": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Syanaputra\\SilverstripeExtraTemplateSyntax\\": "src/"
    }
  },
  "extra": {
    "installer-name": "silverstripe-extra-template-syntax",
    "branch-alias": {
      "dev-main": "2.x-dev",
      "dev-1.x": "1.x-dev"
    }
  }
}